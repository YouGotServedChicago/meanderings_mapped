<form class="form-login" action"/user/login" method="post">
  <div class="container">
    <div class="row" width=50%>
      <div class="col-md-6">
        <label for="user_email">Email address</label>
        <input type="email" name="user_email" class="form-control" id="user_email" placeholder="Email">
      </div>
    </div>
      <br>
      <br>
    <div class="row" width=50%>
      <div class="col-md-6">
        <label for="user_password">Password</label>
        <input type="password" name="password" class="form-control" id="user_password" placeholder="Password">
      </div>
    </div>
      <br>
      <br>
    <button id="user_login" type="submit" class="btn btn-default">Submit</button>
  </div>
</form>
